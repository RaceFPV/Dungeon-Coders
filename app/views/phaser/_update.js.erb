Dude.prototype.update = function() {
        if (this.currentSpeed > 0)
        {
            this.currentSpeed -= 7;
        }
        // check if player is standing on teleporter 1
        if (teleportplayer1(this.dude)) {
          this.dude.body.x = 2690;
          this.dude.body.y = 225;
        }
        // check if player is standing on teleporter 1 home
        if (teleportplayer1home(this.dude)) {
          this.dude.body.x = 300;
          this.dude.body.y = 300;
        }
        // check if player is standing on level 1 success
        if (teleportplayer1success(this.dude)) {
          this.dude.body.x = 6340;
          this.dude.body.y = 560;
        }
        // check if player is standing on level 1 fail
        if (teleportplayer1fail(this.dude)) {
          this.dude.body.x = 2687;
          this.dude.body.y = 225;
        }
        // check if player is standing on level 2 success
        if (teleportplayer2success(this.dude)) {
          this.dude.body.x = 5408;
          this.dude.body.y = 2144;
        }
        // check if player is standing on level 2 fail
        if (teleportplayer2fail(this.dude)) {
          this.dude.body.x = 6340;
          this.dude.body.y = 560;
        }
        // check if player is standing on level 3 success
        if (teleportplayer3success(this.dude)) {
          this.dude.body.x = 300;
          this.dude.body.y = 300;
          this.completedlevels = true;
        }
        // check if player is standing on level 3 fail
        if (teleportplayer3fail(this.dude)) {
          this.dude.body.x = 5408;
          this.dude.body.y = 2144;
        }
        this.dude.body.velocity.x = 0;
        this.dude.body.velocity.y = 0;
};

function update() {

    for (var i in playersList) {
		  if (!playersList[i]) continue;
	  	var curDude = playersList[i].dude;
		  for (var j in playersList) {
			  if (!playersList[j]) continue;
			  if (playersList[j].alive) {
				  playersList[j].update();
			  }
		}
}
